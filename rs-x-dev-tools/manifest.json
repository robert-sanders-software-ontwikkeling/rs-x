{
  "manifest_version": 3,
  "name": "RS-X DevTools",
  "version": "1.0.0",
  "devtools_page": "dev-tools.html",
  "permissions": ["tabs", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["proxies/page-proxy.global.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
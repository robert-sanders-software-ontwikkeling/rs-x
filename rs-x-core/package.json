{
    "name": "@rs-x/core",
    "version": "0.4.0",
    "description": "Core reactive primitives, dependency injection, and state foundations for the RS-X framework",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/robert-sanders-software-ontwikkeling/rs-x"
    },
    "homepage": "https://github.com/robert-sanders-software-ontwikkeling/rs-x",
    "bugs": {
        "url": "https://github.com/robert-sanders-software-ontwikkeling/rs-x/issues"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "scripts": {
        "build": "tsup lib/index.ts --format esm --dts --tsconfig ../tsconfig.build.json",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "fast-equals": "^5.3.2",
        "inversify": "^7.1.0",
        "reflect-metadata": "^0.2.2",
        "lodash.clonedeepwith": "^4.5.0"
    },
    "peerDependencies": {
        "rxjs": "^7.8.2"
    },
    "peerDependenciesMeta": {
        "rxjs": {
            "optional": false
        }
    },
    "devDependencies": {
        "tsup": "^8.5.1",
        "rimraf": "^6.1.2"
    },
    "engines": {
        "node": ">=24"
    },
    "keywords": [
        "rs-x",
        "reactive",
        "state-management",
        "dependency-injection",
        "observable",
        "typescript",
        "framework-core"
    ]
}
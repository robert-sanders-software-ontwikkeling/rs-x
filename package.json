{
  "name": "rs-x",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "build": "pnpm -r run build",
    "build:core": "pnpm --filter @rs-x/core run build",
    "build:state-manager": "pnpm --filter @rs-x/state-manager run build",
    "build:expression-parser": "pnpm --filter @rs-x/expression-parser run build",
    "release:version": "pnpm changeset version",
    "release:publish": "pnpm changeset publish",
    "release": "pnpm changeset && pnpm run release:version && pnpm run build && pnpm run release:publish",
    "test": "cross-env TZ=UTC pnpm jest --runInBand --no-cache --config jest.config.ts",
    "test:coverage": "cross-env TZ=UTC pnpm jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "readme": "pnpm ts-node demo/update-readmes.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@swc/core": "^1.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@ungap/structured-clone": "^1.3.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "html-loader-jest": "^0.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-scss-transform": "^1.0.4",
    "pnpm": "^10.22.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
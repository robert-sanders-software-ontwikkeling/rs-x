{
   "name": "rs-x",
   "private": true,
   "version": "0.0.0",
   "packageManager": "pnpm@10.22.0",
   "scripts": {
      "build": "pnpm -r run build",
      "clean": "rimraf node_modules dist .pnpm-store && pnpm -r exec rimraf node_modules dist",
      "reinstall": "pnpm run clean && pnpm install",
      "build:core": "pnpm --filter @rs-x/core run build",
      "build:state-manager": "pnpm --filter @rs-x/state-manager run build",
      "build:expression-parser": "pnpm --filter @rs-x/expression-parser run build",
      "release:version": "pnpm changeset version",
      "release:publish": "pnpm changeset publish",
      "release": "npm-run-all changeset release:version build:all release:publish",
      "test": "cross-env TZ=UTC pnpm jest --runInBand --no-cache --config jest.config.ts",
      "test:coverage": "cross-env TZ=UTC pnpm jest --coverage",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix",
      "readme": "pnpm ts-node demo/update-readmes.ts"
   },
   "devDependencies": {
      "@changesets/cli": "^2.29.8",
      "@swc/core": "^1.15.10",
      "@types/jest": "^30.0.0",
      "@types/node": "^25.0.10",
      "@typescript-eslint/eslint-plugin": "^8.53.1",
      "@typescript-eslint/parser": "^8.53.1",
      "@ungap/structured-clone": "^1.3.0",
      "cross-env": "^10.1.0",
      "eslint": "^9.39.2",
      "eslint-config-prettier": "^10.1.8",
      "eslint-plugin-import": "^2.32.0",
      "eslint-plugin-prettier": "^5.5.5",
      "jest": "^30.2.0",
      "jest-environment-jsdom": "^30.2.0",
      "jest-transform-stub": "^2.0.0",
      "npm-run-all": "^4.1.5",
      "prettier": "^3.8.1",
      "rimraf": "^6.1.2",
      "ts-jest": "^29.4.6",
      "ts-morph": "^27.0.2",
      "ts-node": "^10.9.2",
      "tsup": "^8.5.1",
      "typescript": "^5.9.3"
   }
}
{
   "name": "rs-x",
   "version": "0.0.1",
   "scripts": {
      "build:core": "cd rs-x-core && pnpm build",
      "build:state-manager": "cd rs-x-state-manager && pnpm build",
      "build:expression-parser": "cd rs-x-expression-parser && pnpm build",
      "build:all": "pnpm build:core && pnpm build:state-manager && pnpm build:expression-parser",
      "publish:core": "cd rs-x-core && pnpm publish --access public",
      "publish:state-manager": "cd rs-x-state-manager && pnpm publish --access public",
      "publish:expression-parser": "cd expression-parser && pnpm publish --access public",
      "release": "pnpm changeset && pnpm changeset version && pnpm build:all && pnpm changeset publish",
      "test": "npx  jest --runInBand --no-cache --config jest.config.ts",
      "test-coverage": "npx jest --coverage",
      "lint": "eslint . --ext .ts",
      "lint:fix": "npm run lint -- --fix"
   },
   "dependencies": {
      "astring": "^1.9.0",
      "espree": "^10.3.0",
      "estraverse": "^5.3.0",
      "fast-equals": "^5.3.2",
      "inversify": "^7.1.0",
      "lodash.clonedeepwith": "^4.5.0",
      "reflect-metadata": "^0.2.2",
      "rxjs": "^7.8.2"
   },
   "devDependencies": {
      "@babel/preset-typescript": "^7.26.0",
      "@changesets/cli": "^2.29.7",
      "@types/astring": "^1.3.0",
      "@types/esprima": "^4.0.6",
      "@types/estree": "^1.0.7",
      "@types/jest": "^29.5.14",
      "@types/node": "^22.13.10",
      "@typescript-eslint/eslint-plugin": "^8.46.4",
      "@typescript-eslint/parser": "^8.46.4",
      "@ungap/structured-clone": "^1.3.0",
      "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
      "es-builder": "^0.5.0",
      "esbuild-runner": "^2.2.2",
      "eslint": "^9.23.0",
      "eslint-config-prettier": "^10.1.1",
      "eslint-define-config": "^2.1.0",
      "eslint-plugin-prettier": "^5.2.3",
      "html-loader-jest": "0.2.1",
      "jest": "^29.7.0",
      "jest-environment-jsdom": "^29.7.0",
      "jest-scss-transform": "1.0.4",
      "jiti": "^2.4.2",
      "jsondiffpatch": "^0.7.3",
      "pnpm": "^10.22.0",
      "prettier": "^3.5.3",
      "pretty-format": "^29.7.0",
      "sass": "^1.85.1",
      "sass-lint": "^1.13.1",
      "ts-jest": "^29.2.6",
      "ts-node": "^10.9.2",
      "tsup": "^8.5.1",
      "typescript": "^5.8.2",
      "tsconfig-paths": "^4.2.0",
      "dedent": "^1.7.0"
   }
}

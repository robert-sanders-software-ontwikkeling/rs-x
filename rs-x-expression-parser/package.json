{
  "name": "@rs-x/expression-parser",
  "version": "0.4.18",
  "description": "Reactive JavaScript expression parser with observable AST integration for RS-X",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/robert-sanders-software-ontwikkeling/rs-x"
  },
  "homepage": "https://github.com/robert-sanders-software-ontwikkeling/rs-x",
  "bugs": {
    "url": "https://github.com/robert-sanders-software-ontwikkeling/rs-x/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsup lib/index.ts --format esm --dts --tsconfig ../tsconfig.build.json",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "rxjs": "^7.8.0"
  },
  "peerDependenciesMeta": {
    "rxjs": {
      "optional": false
    }
  },
  "dependencies": {
    "@rs-x/core": "workspace:*",
    "@rs-x/state-manager": "workspace:*",
    "astring": "^1.9.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "rimraf": "^6.1.2"
  },
  "engines": {
    "node": ">=24"
  },
  "keywords": [
    "expression-parser",
    "reactive",
    "observable",
    "ast",
    "state-management",
    "rs-x",
    "typescript"
  ]
}
